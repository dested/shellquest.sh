  "type": "module",
  "scripts": {
    "dev": "nodemon --watch server --watch server.ts --exec tsx server.ts",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "assets": "tsx scripts/process-assets.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:client": "cross-env NODE_ENV=production BUILD_TARGET=server vite build --outDir dist/app/client",
    "build:server": "cross-env NODE_ENV=production BUILD_TARGET=server vite build --ssr server.ts --outDir dist/entry",
    "serve": "cross-env NODE_ENV=production node ./dist/entry/server.js",
    "console": "tsx ./server/console.ts",
    "prettier": "prettier --write .",
    "clean": "rimraf dist",
    "gen": "prisma generate",
    "prisma-generate": "prisma generate",
    "prisma-migrate": "prisma migrate dev",
    "push": "prisma db push",
    "prisma-db-push": "prisma db push",
    "prisma-format": "prisma format",
    "prisma-snake": "node -e \"const {globSync} = require('glob'); const {execSync} = require('child_process'); const files = globSync('prisma/schema/*.prisma'); files.forEach(file => execSync(`prisma-case-format -f ${file}`, {stdio: 'inherit'}));\"",
    "prisma-studio": "prisma studio",
    "prisma-deploy": "prisma migrate deploy",
    "prisma-seed": "npx prisma db seed",
    "stripe-webhook-tester": "stripe listen --forward-to localhost:9200/webhook/stripe --skip-verify"
  },
